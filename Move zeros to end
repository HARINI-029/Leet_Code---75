void moveZeroes(int* nums, int numsSize) {
    int j =0;
    //filling the arrays with non zero
    for (int i=0;i<numsSize;i++)
    {
        if (nums[i]!=0)
        {
            nums[j] = nums[i];
            j++;
        }
    }
// filling remaining with zeros
    while (j<numsSize)
    {
        nums[j] = 0;
        j++;
    }
}
